@model List<Classes.ExpenseData>

<table id="tbleViewDate" class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Products</th>
            <th>Description</th>
            <th>Date</th>
            <th>Amount</th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach (var exp in Model)
            {
                <tr>
                    <td>@exp.ProductName</td>
                    <td>@exp.Description</td>
                    <td>@exp.Date.ToShortDateString()</td>
                    <td>@exp.Amount</td>
                </tr>
            }
        }
    </tbody>
</table>